{
  "expo": {
    "name": "plyos-native-app",
    "slug": "plyos-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "##FFAFF",
      "packagerOpts": {
        "config": "metro.config.js",
        "sourceExts": [
          "expo.ts",
          "expo.tsx",
          "expo.js",
          "expo.jsx",
          "ts",
          "tsx",
          "js",
          "jsx",
          "json",
          "wasm",
          "svg"
        ]
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#FFFAFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
